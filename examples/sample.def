; Example Windows module definition file
; This demonstrates common .def file constructs

NAME MyLibrary
DESCRIPTION "Example Windows library"
VERSION 1.0

EXPORTS
    ; Simple function export
    MyFunction
    
    ; Function with internal name mapping
    PublicFunction=InternalFunction
    
    ; Function with ordinal
    OrdinalFunction @1
    
    ; Function with ordinal and NONAME
    HiddenFunction @2 NONAME
    
    ; Data export
    MyGlobalData DATA
    
    ; Private function (not exported)
    PrivateFunction @3 PRIVATE
    
    ; Function with both ordinal and internal name
    ComplexFunction=InternalComplexFunction @4
    
    ; Another data export with ordinal
    AnotherData @5 DATA
